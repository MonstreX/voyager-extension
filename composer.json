{
    "name": "monstrex/voyager-extension",
    "description": "Voyager Admin Panel Extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Ovdeyev",
            "email": "fido6080net@gmail.com",
            "homepage": "http://ss-dev.ru"
        }
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/monstrex/voyager-extension",
    "keywords": ["Laravel", "Voyager"],
    "require": {
        "tcg/voyager": "1.4.*",
        "spatie/laravel-medialibrary": "^7.18",
        "illuminate/support": "~6.0|~7.0|~8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "MonstreX\\VoyagerExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonstreX\\VoyagerExtension\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MonstreX\\VoyagerExtension\\VoyagerExtensionServiceProvider"
            ]
        }
    }
}
