{
     "name": "monstrex/voyager-extension",
     "description": "Voyager Admin Panel Extension",
     "license": "MIT",
     "authors": [
       {
         "name": "Yuriy Ovdeyev",
         "email": "fido6080net@gmail.com",
         "homepage": "http://ss-host.ru"
       }
     ],
     "minimum-stability": "dev",
     "homepage": "https://github.com/monstrex/voyager-extension",
     "keywords": [
       "Laravel",
       "Voyager",
       "admin"
     ],
    "require": {
        "tcg/voyager": "^1.6.0",
        "spatie/laravel-medialibrary": "^10.0.0",
        "illuminate/support": "~9.0|~10.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0|~10.0",
        "laravel/framework": "~9.0|~10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "MonstreX\\VoyagerExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonstreX\\VoyagerExtension\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MonstreX\\VoyagerExtension\\VoyagerExtensionServiceProvider"
            ]
        }
    }
}
